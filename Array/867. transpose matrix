class Solution {
    public int[][] transpose(int[][] A) {
        //my solution beats 100%
        int row=A.length,col=A[0].length;
        int[][] a=new int[col][row];
        for(int i=0;i<col;i++)
        {
            for(int j=0;j<row;j++)
            {
                a[i][j]=A[j][i];
            }
        }
        return a;
    }
}
