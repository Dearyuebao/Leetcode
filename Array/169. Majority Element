class Solution {
    public int majorityElement(int[] nums) {
        //my solution beats 81%
        // Arrays.sort(nums);
        // int len=nums.length;
        // for(int i=0;i<len/2;i++)
        // {
        //     if(nums[i]==nums[i+len/2])
        //     {
        //         return nums[i];
        //     }
        // }
        // return nums[len/2];
        //similar but more simple solution 100%
        Arrays.sort(nums);
        int len=nums.length;
        return nums[len/2];
    }
}
